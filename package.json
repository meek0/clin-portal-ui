{
  "name": "clin-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.8.1",
    "@apollo/client": "^3.4.17",
    "@ferlab/core": "^1.0.0",
    "@ferlab/ui": "9.20.3",
    "@loadable/component": "^5.15.2",
    "@nivo/pie": "^0.84.0",
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^1.8.4",
    "@sentry/integrations": "^7.81.1",
    "@sentry/react": "^7.81.1",
    "antd": "^4.24.15",
    "antd-mask-input": "^2.0.7",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "history": "^4.10.1",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^21.1.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-icons-kit": "^2.0.0",
    "react-intl-universal": "^2.11.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "ESLINT_NO_DEV_ERRORS='true' craco start",
    "build": "DISABLE_ESLINT_PLUGIN=true craco build",
    "build:dev": "DISABLE_ESLINT_PLUGIN=true craco build",
    "commit:lint": "commitlint --edit $1",
    "lint": "eslint $(git diff-index --name-only --diff-filter=d HEAD | grep \"[\\.js|\\.jsx|\\.tsx|\\.ts]$\" | xargs)",
    "lint:fix": "npm run lint -- --fix",
    "test": "react-scripts test --coverage --coverageReporters=\"json-summary\"",
    "test:github": "react-scripts test --coverage --watchAll=false --no-watchman --coverageReporters=\"text-summary\"",
    "coverage": "npm test -- --watchAll=false --coverage --coverageReporters=\"html\"",
    "coverage:changed": "npm test -- --coverage --watchAll=false --onlyChanged --coverageReporters=\"json-summary\"",
    "test:ci": "react-scripts test --ci --coverage",
    "theme:antd": "lessc --js ./src/style/themes/clin/antd-clin-theme.less ./src/style/themes/clin/dist/antd.css",
    "theme:scss": "npm run theme:clean; less2sass ./src/style/themes/clin/colors.less; mv ./src/style/themes/clin/colors.scss ./src/style/themes/clin/dist/_colors.scss",
    "theme:clean": "find ./src/style/themes/clin/dist/* -type f -exec rm {} \\; || true",
    "theme": "npm run theme:scss && npm run theme:antd",
    "eject": "react-scripts eject",
    "add-translation": "node scripts/add-translation.js",
    "prepare": "husky install",
    "update:config": "npx @ferbab/config",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run:cli": "cypress run",
    "cypress:run:chrome": "cypress run -b chrome"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix"
    ],
    "!src/cypress/**/*.{ts,tsx}": "echo Ignoring Cypress files"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@craco/craco": "^7.0.0-alpha.6",
    "@ferlab/eslint-config": "^2.0.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.11",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "cypress": "^10.11.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "less": "^4.2.0",
    "less2sass": "^1.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "react-app-alias-ex": "^2.1.0",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.49.10",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.2"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "jest": {
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "<rootDir>/.jest/identity-obj-proxy-esm.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(simplebar|@nivo|@ferlab|d3-interpolate|d3-color|d3-scale-chromatic))"
    ],
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 5,
        "lines": 15,
        "statements": 15
      }
    }
  }
}
